# Classes of creator and contributor GNDs in EconBiz
#
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX gndo: <http://d-nb.info/standards/elementset/gnd#>
#

select (str(count(distinct ?actor)) as ?count) ?class
WHERE {
  graph <http://zbw.eu/beta/ebds/ng> {
    {
      ?pub dcterms:creator ?actor 
    } union {
      ?pub dcterms:contributor ?actor
    }
  }
  graph <http://zbw.eu/beta/gnd/ng> {
    ?actor a ?class .
  }
}
group by ?class
order by desc(count(distinct ?actor))

